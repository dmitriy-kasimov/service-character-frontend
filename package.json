{
  "name": "service-character-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "preview": "vite preview",
    "prettier": "npx prettier --write **/*.{ts,tsx,json}"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.24.1",
    "swiper": "^11.2.6"
  },
  "devDependencies": {
    "@altv/types-client": "^16.2.3",
    "@altv/types-natives": "^16.2.0",
    "@altv/types-shared": "^16.2.1",
    "@altv/types-webview": "^16.2.1",
    "@project-1114/ui-kit": "^0.1.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mega": "^0.0.10",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-unused-imports": "^3.2.0",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.2.0"
  }
